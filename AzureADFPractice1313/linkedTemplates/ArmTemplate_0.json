					{
						"name": "Facility_Name",
						"type": "varchar"
					},
					{
						"name": "Org_PAC_ID",
						"type": "varchar"
					},
					{
						"name": "Num_Org_Members",
						"type": "int",
						"precision": 10
					},
					{
						"name": "Address_Line_1",
						"type": "varchar"
					},
					{
						"name": "Address_Line_2",
						"type": "varchar"
					},
					{
						"name": "Address_Line_2_Supplement",
						"type": "varchar"
					},
					{
						"name": "City_Town",
						"type": "varchar"
					},
					{
						"name": "State",
						"type": "char"
					},
					{
						"name": "ZIP_Code",
						"type": "varchar"
					},
					{
						"name": "Telephone_Number",
						"type": "varchar"
					},
					{
						"name": "Ind_Assignment",
						"type": "char"
					},
					{
						"name": "Grp_Assignment",
						"type": "char"
					},
					{
						"name": "Address_ID",
						"type": "varchar"
					}
				],
				"typeProperties": {
					"schema": "dbo",
					"table": "ProviderInformation"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureSqlDatabase1')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/DelimitedText1')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureBlobStorage1",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": "DAC_NationalDownloadableFile.csv",
						"container": "healthcare-provider-input"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "NPI",
						"type": "String"
					},
					{
						"name": "Ind_PAC_ID",
						"type": "String"
					},
					{
						"name": "Ind_enrl_ID",
						"type": "String"
					},
					{
						"name": "Provider Last Name",
						"type": "String"
					},
					{
						"name": "Provider First Name",
						"type": "String"
					},
					{
						"name": "Provider Middle Name",
						"type": "String"
					},
					{
						"name": "suff",
						"type": "String"
					},
					{
						"name": "gndr",
						"type": "String"
					},
					{
						"name": "Cred\t\t\t\t",
						"type": "String"
					},
					{
						"name": "Med_sch",
						"type": "String"
					},
					{
						"name": "Grd_yr",
						"type": "String"
					},
					{
						"name": "pri_spec",
						"type": "String"
					},
					{
						"name": "sec_spec_1",
						"type": "String"
					},
					{
						"name": "sec_spec_2",
						"type": "String"
					},
					{
						"name": "sec_spec_3",
						"type": "String"
					},
					{
						"name": "sec_spec_4",
						"type": "String"
					},
					{
						"name": "sec_spec_all",
						"type": "String"
					},
					{
						"name": "Telehlth\t\t\t\t",
						"type": "String"
					},
					{
						"name": "Facility Name",
						"type": "String"
					},
					{
						"name": "org_pac_id",
						"type": "String"
					},
					{
						"name": "num_org_mem",
						"type": "String"
					},
					{
						"name": "adr_ln_1",
						"type": "String"
					},
					{
						"name": "adr_ln_2",
						"type": "String"
					},
					{
						"name": "ln_2_sprs",
						"type": "String"
					},
					{
						"name": "City/Town",
						"type": "String"
					},
					{
						"name": "State",
						"type": "String"
					},
					{
						"name": "ZIP Code",
						"type": "String"
					},
					{
						"name": "Telephone Number",
						"type": "String"
					},
					{
						"name": "ind_assgn",
						"type": "String"
					},
					{
						"name": "grp_assgn",
						"type": "String"
					},
					{
						"name": "adrs_id",
						"type": "String"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureBlobStorage1')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/pipeline1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Extarct CSV file to ADF_Practice_DB_1",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "0.00:10:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "DelimitedTextSource",
								"storeSettings": {
									"type": "AzureBlobStorageReadSettings",
									"recursive": true,
									"enablePartitionDiscovery": false
								},
								"formatSettings": {
									"type": "DelimitedTextReadSettings"
								}
							},
							"sink": {
								"type": "AzureSqlSink",
								"writeBehavior": "insert",
								"sqlWriterUseTableLock": false
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "DelimitedText1",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "AzureSqlTable1",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/DelimitedText1')]",
				"[concat(variables('factoryId'), '/datasets/AzureSqlTable1')]"
			]
		}
	]
}